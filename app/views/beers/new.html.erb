<%= form_for @beer do |f| %>
  <%= f.label :brewery %>
  <%= f.text_field :brewery %>

  <%= f.label :brew %>
  <%= f.text_field :brew %>

  <%= f.label :year %>
  <%= f.text_field :year %>

  <%= f.label :style %>
  <%= f.text_field :style %>

  <%= f.label :abv %>
  <%= f.text_field :abv %>

  <%= f.submit %>

<% end %>